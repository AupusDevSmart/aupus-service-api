  a3a_verde_tusd_p  Decimal? @db.Decimal(10, 6)
  a3a_verde_tusd_fp Decimal? @db.Decimal(10, 6)
  a3a_verde_te_d    Decimal? @db.Decimal(10, 6)
  a3a_verde_te_p    Decimal? @db.Decimal(10, 6)
  a3a_verde_te_fp   Decimal? @db.Decimal(10, 6)

