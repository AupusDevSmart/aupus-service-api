  qualidade       String?  @db.VarChar(20)
  created_at      DateTime @default(now()) @db.Timestamp(0)

  // Campos para agregação de 1 minuto
  timestamp_fim DateTime? @db.Timestamp(0)
  num_leituras  Int? // Número de leituras agregadas no período

  // ✅ NOVOS CAMPOS - PHF e Energia (GDD v2.0)
  phf_atual          Decimal? @db.Decimal(12, 3) // Period Hour Forward atual (kWh acumulado)
  phf_anterior       Decimal? @db.Decimal(12, 3) // PHF da leitura anterior
  energia_kwh        Decimal? @db.Decimal(10, 3) // Energia consumida (delta PHF)
  potencia_ativa_kw  Decimal? @db.Decimal(10, 3) // Potência ativa (Pa+Pb+Pc) para demanda
  tipo_horario       String?  @db.VarChar(20)    // PONTA, FORA_PONTA, HORARIO_RESERVADO

  equipamento equipamentos @relation(fields: [equipamento_id], references: [id], onDelete: Cascade)

  @@unique([equipamento_id, timestamp_dados], name: "uk_equipamento_timestamp")
  @@index([equipamento_id])
  @@index([timestamp_dados])
  @@index([equipamento_id, timestamp_dados])
  @@index([equipamento_id, timestamp_dados, energia_kwh], name: "idx_equipamentos_dados_periodo")
  @@index([equipamento_id, potencia_ativa_kw], name: "idx_equipamentos_dados_potencia")
  @@index([equipamento_id, tipo_horario, timestamp_dados], name: "idx_equipamentos_dados_tipo_horario")
  @@map("equipamentos_dados")
}

